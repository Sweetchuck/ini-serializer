{
    "name": "sweetchuck/ini-serializer",
    "description": "INI reader-writer",
    "license": "proprietary",
    "config": {
        "bin-dir": "bin",
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": {},
    "require": {
        "php": ">=7.1",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "codeception/codeception": "^2.5",
        "sweetchuck/git-hooks": "^0.0",
        "sweetchuck/robo-git": "^0.0",
        "sweetchuck/robo-phpcs": "^0.0",
        "sweetchuck/robo-phpmd": "^0.0"
    },
    "autoload": {
        "psr-4": {
            "Sweetchuck\\IniSerializer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sweetchuck\\IniSerializer\\Tests\\Unit\\": "tests/unit/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "@deploy-git-hooks"
        ],
        "post-update-cmd": [
            "@deploy-git-hooks"
        ],
        "deploy-git-hooks": "\\Sweetchuck\\GitHooks\\Composer\\Scripts::deploy"
    }
}
